<h1>User Page</h1>
{{#each user}}
    <div>
        <div>{{this.photo}}</div>
         <div>Name: <a href="/users/{{this._id}}"></a>{{this.username}}</div> 
         <div><a href="/users/{{this._id}}/bio"></a>Bio: {{this.bio}}</div> 
         <div>{{this.username}}'s <a href="/brewery/{{../this._id}}/beer">Beers</a></div> 
         <a href="/users/{{this._id}}/delete">Delete {{this.username}}</a> 
         <a href="/users/{{this._id}}/edit">Edit {{this.username}}</a> 
    </div>
{{/each}}
